{
    "name": "utkarshgayguwal/log-management",
    "description": "A reusable Laravel package for comprehensive log management with template support and bulk logging capabilities",
    "type": "laravel-package",
    "keywords": ["laravel", "log", "management", "audit", "logging", "template", "bulk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Utkarsh Gayguwal",
            "email": "utkarsh@example.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "illuminate/support": "^5.3|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "kblais/laravel-query-filter": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "UtkarshGayguwal\\LogManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UtkarshGayguwal\\LogManagement\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UtkarshGayguwal\\LogManagement\\Providers\\LogManagementServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}